{
    "name": "horologyhub/modbuilder",
    "description": "Platform for watch enthusiasts and Seiko Mod builders",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.8",
        "symfony/dom-crawler": "^6.4",
        "symfony/css-selector": "^6.4",
        "monolog/monolog": "^3.5",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.92"
    },
    "autoload": {
        "psr-4": {
            "HorologyHub\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Antigravity",
            "email": "antigravity@example.com"
        }
    ],
    "config": {
